
/*
--author: @jkopacko
+= Descriptive names: N/A
+= Variable type: N/A
+= Value: N/A
+= Version 1.0 - N/A
+= Query type: Live Discover
+= OS Support: Windows
*/

SELECT
   name as 'Key Name',
   source as 'Start Up source',
   path as 'Path',
   args as 'Aruments',
   username as 'Owner',
   status as 'Status'
FROM startup_items
ORDER by status
